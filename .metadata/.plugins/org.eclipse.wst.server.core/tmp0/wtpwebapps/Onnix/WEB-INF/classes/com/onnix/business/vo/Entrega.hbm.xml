<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.onnix.business.vo.EntregaVO" table="entrega" >
 
 		<id name="idEntrega"	type="long" column="idEntrega">
 			<generator class="native"/>
 		</id>
 		
		<property name="fecha" 				type="date" 	column="fecha"/>
		<property name="cantidad" 			type="float" 	column="entrega"/>
		<property name="idCuenta" 			type="long"		column="idCuenta"/>
    </class>
</hibernate-mapping>